{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "semi": "off",
    "operator-linebreak": "off",
    // "no-console": "warn",
    "no-console": "off",
    "no-debugger": "off",
    "no-unused-vars": "warn",
    "max-len": ["error", {
        "code": 120,
        "ignoreTemplateLiterals": true
      }
    ]
    ,
    "emitWarning": "off",
    "no-trailing-spaces": ["off"]
  },
  "extends": [
    "eslint:recommended",
    "google"]
}